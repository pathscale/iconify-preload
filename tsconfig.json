{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"lib": [
			"ES2022"
		],
		"declaration": true,
		"emitDeclarationOnly": false,
		"esModuleInterop": true,
		"moduleResolution": "NodeNext",
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src"
	]
}
