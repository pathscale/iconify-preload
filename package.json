{
	"name": "rsbuild-plugin-iconify",
	"version": "1.0.2",
	"description": "RSBuild plugin for optimized Iconify SVG icons compilation",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bun run build:sources && bun run build:types",
		"build:sources": "bun build ./src/index.ts --outdir ./dist --target node --format esm --minify --sourcemap=external",
		"build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
		"prepublishOnly": "bun run build"
	},
	"homepage": "https://github.com/pathscale/iconify-preload",
	"repository": {
		"type": "git",
		"url": "https://github.com/pathscale/iconify-preload.git"
	},
	"keywords": [
		"rsbuild",
		"rspack",
		"iconify",
		"svg",
		"icons",
		"solidjs",
		"optimization"
	],
	"author": "Ayanov Dulat <ayanov.dulat@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@iconify/json": "^2.2.x",
		"@iconify/utils": "^2.3.x",
		"@rspack/core": "^1.3.13",
		"cssnano": "^7.0.x",
		"postcss": "^8.5.x",
		"postcss-cli": "^11.0.x",
		"svgo": "^3.3.x"
	},
	"peerDependencies": {
		"@rsbuild/core": "^1.x",
		"@rslib/core": "^0.9.x"
	},
	"devDependencies": {
		"@types/node": "^20.x"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	]
}
